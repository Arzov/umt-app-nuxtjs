{
  "name": "umt-app-nuxtjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NUXT_HOST=0.0.0.0 NUXT_PORT=3000 nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate && npx cap copy",
    "generate:hot-reload": "nuxt-ts generate && npx cap copy",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "test": "jest",
    "build:android": "npx cap add android",
    "build:ios": "npx cap open ios",
    "open:android": "npx cap open android",
    "open:ios": "npx cap add ios",
    "open:web": "npx cap serve"
  },
  "dependencies": {
    "@capacitor/android": "2.4.6",
    "@capacitor/cli": "^2.4.6",
    "@capacitor/core": "^2.4.6",
    "@capacitor/ios": "2.4.6",
    "@nuxt/typescript-runtime": "^2.0.1",
    "core-js": "^3.8.2",
    "nuxt": "^2.14.12"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.12",
    "@nuxt/typescript-build": "^2.0.4",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.4.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "vue-jest": "^3.0.4"
  }
}
